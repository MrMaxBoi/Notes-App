{
  "name": "notes-app",
  "version": "1.0.0",
  "description": "A full-stack notes management application built with Vue.js, Spring Boot, and SQLite",
  "scripts": {
    "dev:backend": "cd backend-spring && mvn spring-boot:run",
    "dev:frontend": "cd frontend && npm run dev",
    "build:backend": "cd backend-spring && mvn clean package",
    "build:frontend": "cd frontend && npm run build",
    "build": "npm run build:backend && npm run build:frontend",
    "start": "java -jar backend-spring/target/notes-app-1.0.0.jar"
  },
  "keywords": ["notes", "spring-boot", "vue", "java", "sqlite"],
